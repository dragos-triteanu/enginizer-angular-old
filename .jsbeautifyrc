{
    "html": {
        "indent_size": 4,
        "wrap_attributes": "force-aligned",
        "preserve_newlines": true,
        "wrap_attributes_indent_size": 2,
        "unformatted": [
            "a",
            "abbr",
            "area",
            "audio",
            "b",
            "bdi",
            "bdo",
            "br",
            "canvas",
